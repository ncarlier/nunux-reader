    "grunt-bower-task": "~0.3.4",
    "grunt-mocha-test": "~0.8.1",
    "grunt-contrib-jshint": "~0.8.0",
    "grunt-contrib-uglify": "~0.3.2",
    "grunt-contrib-copy": "~0.5.0",
