    "ejs": "*",
    "express": "3.4.x",
    "feedparser": "0.15.x",
    "less-middleware": "0.1.x",
    "newrelic": "*",
    "node-keeper": "0.2.x",
    "opmlparser": "0.5.x",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "redis": "^2.5.3",
    "request": "2.27.x",
    "underscore": "~1.5.2",
    "when": "~3.0.0"
