    "template": {
      "withCompare": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ncarlier/nunux-reader.git"
  },
  "scripts": {
    "start": "node server/app | bunyan",
    "debug": "node-inspector & node --debug server/app",
