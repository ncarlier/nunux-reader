{
  "author": "Nicolas Carlier (https://github.com/ncarlier/)",
  "name": "nunux-reader",
  "version": "1.0.0",
  "description": "The super-fast-minimalist-nosql-opensource Google Reader revival.",
  "license": "gplv3",
  "homepage": "http://reader.nunux.org",
  "apidoc": {
    "title": "Nunux Reader API documentation",
